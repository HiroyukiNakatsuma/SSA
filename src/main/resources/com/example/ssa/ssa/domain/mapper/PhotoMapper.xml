<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.ssa.ssa.domain.mapper.PhotoMapper">
    <insert id="insert">
        INSERT INTO
        Photo (
            roomId,
            imagePath,
            postedAccountId,
            createdAt,
            updatedAt
        ) VALUES (
            #{roomId},
            #{imagePath},
            #{postedAccountId},
            now(),
            now()
        )
    </insert>
</mapper>