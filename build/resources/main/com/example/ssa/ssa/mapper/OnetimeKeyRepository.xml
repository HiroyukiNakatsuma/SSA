<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.ssa.ssa.mapper.OnetimeKeyRepository">
    <insert id="insert">
        INSERT INTO
        OnetimeKey (
            onetimeKey,
            roomId,
            createdAccountId,
            createdAt,
            updatedAt
        ) VALUES (
            #{randomKey},
            #{roomId},
            #{createdAccountId},
            now(),
            now()
        )
    </insert>
</mapper>